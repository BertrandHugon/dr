avec le script Javascript
 
<script type="text/javascript">
function changeId(id) {
var img = document.getElementById("carte"); 
if (id != ""){img.src = "IMAGE/carte_" + id + ".png";}
else {img.src = "IMAGE/carte.png";}
};
</script>
 
